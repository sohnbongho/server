<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <BaseOutputPath></BaseOutputPath>
	 <NoWarn>$(NoWarn);CS8602;CS8600;CS8625;CS8618;CS8601;CS8603;CS8604</NoWarn>
  </PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
		<OutputPath>../Binary/Debug/Library</OutputPath>
		<DefineConstants>;TRACE;USE_TASK</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<OutputPath>../Binary/Release/Library</OutputPath>
		<DefineConstants>;USE_TASK</DefineConstants>
	</PropertyGroup>


	<PropertyGroup>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<RootNamespace>Library</RootNamespace>
	</PropertyGroup>


	<ItemGroup>
	  <Compile Remove="Tools\Message.cs" />
	</ItemGroup>


	<ItemGroup>
	  <None Include="Tools\Message.cs" />
	</ItemGroup>


	<ItemGroup>
	  <PackageReference Include="Google.Protobuf" Version="3.23.2" />
	</ItemGroup>
</Project>
